<% 
if(typeof online != 'undefined'){ 
if(online != false){
%>
<table class="ranking-table">
	<thead>
	<tr class="main-tr">
		<th style="text-align:center;">#</th>
		<th style="text-align:center;"><%= App.lc.translate('Name').fetch() %></th>
		<th style="text-align:center;"><%= App.lc.translate('Online Time').fetch() %></th>
	</tr>
	</thead>
	<tbody>
	<% for(var i=0; i<online.length; i++) { %>
	<tr>
		<td style="text-align:center;"><%= i+1 %> <% if(config.online.display_country == 1) { %><span class="f16"><span class="flag <%= online[i].country %>"></span></span><% } %></td>
		<td><a href="<%= base_url %>character/<%= online[i].name_hex %>/<%= server_selected %>"><%= online[i].name %></a></td>
		<td><%= online[i].h %> <%= App.lc.translate('Hours').fetch() %> <%= online[i].m %> <%= App.lc.translate('Minutes').fetch() %></td>
	</tr>  
	<% } %>
	</tbody>
</table>  
<% if(typeof cache_time != 'undefined'){ %>
<div style="text-align: right;padding-top: 15px;"><%= cache_time %></div>
<% } %>
<% 
}
else{
%>
<div style="padding: 0 30px 0px 50px;"><div class="i_note"><%= App.lc.translate('No Players Found').fetch() %></div></div>
<%
}
} 
%>